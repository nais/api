interface Workload {
  id: ID!
  name: String!
  image: String!
  imageDetails: ImageDetails!
  deployInfo: DeployInfo!
  env: Env!
  accessPolicy: AccessPolicy!
  status: WorkloadStatus!
  authz: [Authz!]!
  variables: [Variable!]!
  resources: Resources!
  type: WorkloadType!
}

enum WorkloadType {
  APP
  NAISJOB
}
