mutation {
	setTeamMemberRole(
		input: {
			teamSlug: "{{ .teamSlug }}",
			userEmail: "email-1@example.com",
			role: MEMBER
		}
	) {
		member {
			role
		}
	}
}

RETURNS

{
	"data": null,
	"errors": [
		{
			"message": "User is not a member of the team.",
			"path": [
				"setTeamMemberRole"
			]
		}
	]
}
