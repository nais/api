mutation {
	addTeamMember(input:{
		teamSlug: "{{ .teamSlug }}"
		userEmail: "doesnotexist@example.com"
		role: MEMBER
  	}) {
		member {
			user {
				email
			}
		}
  	}
}

RETURNS

{
	"data": null,
	"errors": [
		{
			"message": "The specified user was not found.",
			"path": [
				"addTeamMember"
			]
		}
	]
}
