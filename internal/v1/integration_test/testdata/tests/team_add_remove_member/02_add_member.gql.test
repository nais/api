mutation {
	addTeamMember(input:{
    	teamSlug: "{{ .teamSlug }}"
    	userEmail: "email-2@example.com"
    	role: MEMBER
  	}) {
    	correlationID
    	user {
      		email
      		name
    	}
    	team {
      		members {
        		nodes {
          			role
          			user {
            			email
            			name
          			}
        		}
      		}
    	}
  	}
}

RETURNS

OPTION data.addTeamMember.correlationID=IGNORE

ENDOPTS

{
	"data": {
    	"addTeamMember": {
      		"correlationID": "something",
      		"team": {
