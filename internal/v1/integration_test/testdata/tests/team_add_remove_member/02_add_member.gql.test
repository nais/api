              				"role": "OWNER",
              				"user": {
                				"email": "authenticated@example.com",
                				"name": "Authenticated User"
              				}
            			},
            			{
              				"role": "MEMBER",
              				"user": {
                				"email": "email-2@example.com",
                				"name": "name-2"
              				}
            			}
          			]
        		}
      		},
      		"user": {
        		"email": "email-2@example.com",
        		"name": "name-2"
      		}
    	}
  	}
}
