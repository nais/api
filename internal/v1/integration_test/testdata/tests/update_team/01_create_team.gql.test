mutation {
  createTeam(
    input: {slug: "someteamname", purpose: "some purpose", slackChannel: "#channel"}
  ) {
    team {
      slug
    }
  }
}

RETURNS

{
  "data": {
    "createTeam": {
      "team": {
        "slug": "someteamname"
      }
    }
  }
}

STORE teamSlug=data.createTeam.team.slug
