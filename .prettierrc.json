{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "all",
	"printWidth": 100,
	"plugins": ["prettier-plugin-sql"],
	"overrides": [
		{
			"files": "*.sql",
			"options": {
				"language": "postgresql",
				"paramTypes": "{ named: ['@'] }",
				"functionCase": "upper",
				"keywordCase": "upper",
				"dataTypeCase": "upper",
				"newlineBeforeSemicolon": true
			}
		}
	]
}
